[uwsgi]
socket = {{ base_dir }}/uwsgi.sock
chmod-socket = 775
chdir = {{ site_dir }}
master = true
virtualenv = {{ base_dir }}
module =  {{ site_name }}.wsgi:application
uid = {{ uid }}
gid = {{ gid }}
processes = {{ processes }}
plugins = python3,logfile
logger = file:{{ base_dir }}/uwsgi.log
env = DJANGO_SETTINGS_MODULE={{ site_name }}.settings
